    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch via PNPM",
        "request": "launch",
        "type": "node",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "pnpm",
        "runtimeArgs": [
            "start"
        ],
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome against 127.0.0.1:3000",
        "url": "http://127.0.0.1:3000",
        "webRoot": "${workspaceRoot}",
      }
    ]
  }
  